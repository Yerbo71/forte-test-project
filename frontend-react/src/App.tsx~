import './App.css';
import { AppLayout } from './layout/appLayout.tsx';
import { BrowserRouter, Route, Routes } from 'react-router-dom';
import ContactsTablePage from '@/pages/contacts/contacts-table/contactsTablePage.tsx';
import ContactsFormPage from '@/pages/contacts/contacts-form/contactsFormPage.tsx';

function App() {
  return (
      <BrowserRouter>
        <AppLayout>
        <Routes>
          <Route path="/" element={<ContactsTablePage />} />
          <Route path="/contact-form" element={<ContactsFormPage />} />
          <Route path="/contact-form/:id" element={<ContactsFormPage />} />
        </Routes>
        </AppLayout>
      </BrowserRouter>
  );
}

export default App;
