import axios from 'axios';
import { CONTACTS_URL } from '@/api/url';
import { IContactInputData } from '@/api/types';

export const postContact = async (data: IContactInputData): Promise<IPostPutResponse> => {
  const response = await axios.post<IPostPutResponse>(CLIENT_URL, data, {
    headers: { 'Content-Type': 'application/json' }
  });
  return response.data;
};
